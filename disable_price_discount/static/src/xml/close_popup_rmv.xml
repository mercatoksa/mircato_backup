<?xml version="1.0" encoding="UTF-8"?>
<templates id="temp2" xml:space="preserve">
   <t t-inherit="point_of_sale.HeaderButton" t-inherit-mode="extension">
      <xpath expr="//div[@class='header-button']" position="replace">
           <div class="header-button" t-on-click="this.canCloseSession()"> <span><i class="fa fa-sign-out" role="img"/> <t t-if="!env.isMobile">Close</t></span>
           </div>
      </xpath>
   </t>
</templates>


